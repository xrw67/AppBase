    class PluginManager(info, list, load, unload), 
    struct thefox_plugin_t p4_plugin {
      char *name = "p4'
      u32 version = PLUGIN_VERSION(0,1,0);
      char *depends = "p1,p2,p3"
      init_fn_t init = p4_init
      exit_fn_t exit = p5_exit
    }
    EXPORT_PLUGIN_ENTRY(p4_plugin);


